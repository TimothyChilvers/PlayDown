{
  "manifest_version": 2,

  "name": "PlayDown",
  "description": "This extension downloads the current page as an Xcode Playground",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Moo!"
  },
  
  "permissions": [
    "activeTab",
    "http://playdown.herokuapp.com/",
    "https://ajax.googleapis.com/"
	
  ]
}
